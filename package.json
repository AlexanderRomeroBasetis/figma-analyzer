{
  "name": "figma-analyzer",
  "version": "1.0.0",
  "description": "Herramienta para analizar y conectarse a la API de Figma",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import ./register.mjs src/figma-connection-checker.ts",
    "test": "node --import ./register.mjs src/figma-api-test.ts",
    "inspect": "node --import ./register.mjs src/figma-json-inspector.ts",
    "build": "tsc",
    "dev": "node --import ./register.mjs src/figma-connection-checker.ts",
    "front": "cd src/front && npm run dev",
    "front:build": "cd src/front && npm run build"
  },
  "keywords": ["figma", "api", "typescript", "analyzer"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
